{
  "name": "undectr",
  "version": "2.0.0",
  "description": "Professional AI Audio Mastering - Remove watermarks and master audio for streaming platforms",
  "homepage": "https://undectr.com",
  "author": "Undectr Team",
  "license": "UNLICENSED",
  "main": "dist-electron/main.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build:web": "vite build",
    "build": "vite build && electron-builder",
    "build:win": "vite build && electron-builder --win",
    "build:mac": "vite build && electron-builder --mac",
    "build:linux": "vite build && electron-builder --linux",
    "preview": "vite preview"
  },
  "keywords": [
    "audio mastering",
    "ai audio",
    "watermark removal",
    "spotify ready",
    "lufs",
    "true peak",
    "electron"
  ],
  "build": {
    "appId": "com.undectr.app",
    "productName": "Undectr",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "portable",
      "icon": "icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.png",
      "category": "public.app-category.music",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.png"
    }
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "dependencies": {
    "fft.js": "^4.0.4",
    "wavesurfer.js": "^7.12.1"
  }
}